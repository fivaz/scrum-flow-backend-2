{
  "builds": [
    {
      "src": "scrumflow5/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "scrumflow5/wsgi.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "public/**"
    }
  }
}